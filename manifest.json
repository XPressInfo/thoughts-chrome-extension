{
  "name": "Thought of the new-tab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Provides a new thought every time a new tab opens.",
  "homepage_url": "https://github.com/nirajpandkar/thoughts-chrome-extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  }
}